<!DOCTYPE html>
<html>
  <head>
    <%= render "shared/head" %>
  </head>

  <body>
    <%= render "shared/navbar" %>

    <main class="container">
      <% flash.each do |key, value| %>
        <div class="flash-<%= key %>">
          <%= value %>
        </div>
      <% end %>

      <%= yield %>
    </main>

    <%= render "shared/footer" %>
  </body>
</html>